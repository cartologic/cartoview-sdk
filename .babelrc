{
    "presets": [
      [ "env", { "targets": { "uglify": true } } ],
    "es2015",
    "react",
    "stage-2"
  ],
    "plugins": [ "transform-object-rest-spread",
      "transform-react-constant-elements",
      [ "transform-class-properties", { "spec": true } ],
      [ "transform-es2015-classes", {
            "loose": true
      } ] ]
}
